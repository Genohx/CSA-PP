public class Hakuna {
    public static void main(String args[]) {
      String text = new String("Hakuna matata it means no worries for the rest of your days");
      String worries, replace;
      
      worries = text.substring(23, 33);
      replace = text.replace('a', '4');
      
      System.out.println(worries);
      System.out.println(replace);
    }//end method main
}//end class Hakuna
